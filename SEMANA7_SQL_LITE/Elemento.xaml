<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="SEMANA7_SQL_LITE.Elemento">
    <ContentPage.Content>
        <StackLayout Spacing="10" Padding="10" >
            <Label x:Name="mensaje" />
            <Entry x:Name="Nombre" Placeholder="Nombre" />
            <Label x:Name="Usuario"></Label>
            <Entry x:Name="usuario" Placeholder="usuario" />
            <Label x:Name="Contraseña"></Label>
            <Entry x:Name="Contrasenia" Placeholder="Contraseña" />
            <Button x:Name="btn_actualizar" Text="Actualizar" Clicked="btn_actualizar_Clicked"></Button>
            <Button x:Name="btn_eliminar" Text="Eliminar" Clicked="btn_eliminar_Clicked"></Button>

        </StackLayout>
    </ContentPage.Content>
</ContentPage>